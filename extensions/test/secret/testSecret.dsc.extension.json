{
    "$schema": "https://aka.ms/dsc/schemas/v3/bundled/extension/manifest.json",
    "type": "Test/Secret",
    "version": "0.1.0",
    "description": "Example secret extension for testing.",
    "secret": {
        "executable": "pwsh",
        "args": [
            "-NoLogo",
            "-NonInteractive",
            "-NoProfile",
            "-Command",
            "./secret.ps1",
            {
                "nameArg": "-Name"
            },
            {
                "vaultArg": "-Vault"
            }
            ]
    }
}
